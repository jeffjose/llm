  },
  "keywords": ["llm", "llama.cpp", "ai", "language-model", "inference", "local", "offline"],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjose/llm.git"
  },
  "type": "commonjs",
  "dependencies": {
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "node-llama-cpp": "^3.10.0",
    "typescript": "^5.8.3"
  }
}
